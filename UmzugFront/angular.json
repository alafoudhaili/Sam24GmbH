{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "35c2a030-70e4-44b0-bcac-3c33ef95e5a5"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "velzon": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/velzon",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
  { "glob": "**/*", "input": "src/assets",      "output": "assets"      },
  { "glob": "**/*", "input": "src/assetsFront", "output": "assetsFront" },
  { "glob": "favicon.ico", "input": "src",      "output": "/"           }
],
            "styles": [
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/owl.theme.default.min.css",
              "src/assets/css/magnific-popup.min.css",
              "src/assets/css/animate.min.css",
              "src/assets/css/boxicons.min.css",
              "src/assets/css/flaticon.css",
              "src/assets/css/meanmenu.min.css",
              "src/assets/css/nice-select.min.css",
              "src/assets/css/odometer.min.css",
              "src/assets/css/dark.css",
              "src/assets/css/style.scss",
              "src/assets/css/responsive.css",
              "node_modules/shepherd.js/dist/css/shepherd.css",
              "node_modules/flatpickr/dist/flatpickr.css",
              "src/styles.scss",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/@ctrl/ngx-emoji-mart/picker.css",
              "src/assets/scss/config/default/bootstrap.scss",
              "node_modules/swiper/swiper-bundle.css",
              "src/assets/scss/config/default/app.scss",
              "src/assets/scss/icons.scss"
            ],

            "scripts": [
            "node_modules/jquery/dist/jquery.min.js",
    
    "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
    
    "src/assetsFront/js/meanmenu.min.js",
    "src/assetsFront/js/nice-select.min.js", 
    "src/assetsFront/js/magnific-popup.min.js",
    "src/assetsFront/js/owl.carousel.min.js",
  "node_modules/html2pdf.js/dist/html2pdf.bundle.min.js"

            ],


            "allowedCommonJsDependencies": [
              "ev-emitter/ev-emitter",
              "can-use-dom",
              "core-js/modules/es.array.filter.js",
              "core-js/modules/es.array.iterator.js",
              "core-js/modules/es.array.reduce.js",
              "core-js/modules/es.function.name.js",
              "core-js/modules/es.object.assign.js",
              "core-js/modules/es.object.to-string.js",
              "core-js/modules/es.parse-int.js",
              "core-js/modules/es.regexp.exec.js",
              "core-js/modules/es.string.iterator.js",
              "core-js/modules/es.string.match.js",
              "core-js/modules/es.string.replace.js",
              "core-js/modules/es.weak-map.js",
              "core-js/modules/web.dom-collections.iterator.js",
              "lodash.debounce",
              "lodash.memoize",
              "lodash.throttle",
              "rxjs/internal/operators",
              "moment",
              "lottie-web",
              "ngx-lightbox",
              "ngx-masonry",
              "leaflet",
              "sweetalert2",
              "lodash",
              "@ckeditor/ckeditor5-build-classic",
              "ngx-csv/ngx-csv",
              "dropzone",
              "file-saver",
              "jquery",
              "meanmenu",
              "owl.carousel",
              "jquery-nice-select",
              "magnific-popup"
           ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "8mb",
                  "maximumError": "8mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "optimization": {
                "scripts": true,
                "styles": {
                    "minify": true,
                    "inlineCritical": false
                }
            }
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "velzon:build:production"
            },
            "development": {
              "browserTarget": "velzon:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "velzon:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
